{
  "name": "anonchat",
  "version": "0.0.0",
  "scripts": {
    "tailwind:build": "NODE_ENV=production npx tailwindcss-cli@latest build src/**/*.pcss --ext css --dir generated/",
    "postcss:build": "postcss src/**/*.pcss --ext scss --dir generated/",
    "postcss:watch": "postcss -w src/**/*.pcss --ext scss --dir generated/",
    "prestart": "npm run postcss:build && npm run clean",
    "watch-lib:signal": "ng build @anonchat/signal --watch",
    "prebuild": "NODE_ENV=production npm run postcss:build",
    "ng": "ng",
    "start": "ng serve & concurrently \"npm run postcss:watch\" \"npm run watch-lib:*\" ",
    "build": "NODE_ENV=production ng build --prod --base-href /app",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prepare": "husky install",
    "clean": "rimraf dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.9",
    "@angular/common": "~11.0.9",
    "@angular/compiler": "~11.0.9",
    "@angular/core": "~11.0.9",
    "@angular/forms": "~11.0.9",
    "@angular/platform-browser": "~11.0.9",
    "@angular/platform-browser-dynamic": "~11.0.9",
    "@angular/router": "~11.0.9",
    "@privacyresearch/libsignal-protocol-typescript": "0.0.12",
    "autoprefixer": "^10.2.3",
    "dexie": "^3.0.3",
    "localforage": "^1.9.0",
    "postcss": "^8.2.4",
    "postcss-short": "^5.0.0",
    "rxjs": "~6.6.0",
    "socket.io-client": "2.3.0",
    "tailwindcss": "^2.0.2",
    "tslib": "^2.0.0",
    "unique-names-generator": "^4.4.0",
    "url-join": "^4.0.1",
    "uuid": "^3.4.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.7",
    "@angular/cli": "~11.0.7",
    "@angular/compiler-cli": "~11.0.9",
    "@types/emscripten": "^1.39.4",
    "@types/jasmine": "~3.6.0",
    "@types/localforage": "^0.0.34",
    "@types/node": "^12.11.1",
    "@types/socket.io-client": "^1.4.35",
    "@types/url-join": "^4.0.0",
    "angular-cli-ghpages": "^1.0.0-rc.1",
    "codelyzer": "^6.0.0",
    "husky": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^11.0.0",
    "ng-packagr": "^11.0.0",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "ng lint --fix"
    ]
  }
}
