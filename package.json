{
  "name": "chat-app",
  "version": "0.0.0",
  "scripts": {
    "tailwind:build": "NODE_ENV=production npx tailwindcss-cli@latest build src/**/*.pcss --ext css --dir generated/",
    "postcss:build": "postcss src/**/*.pcss --ext scss --dir generated/",
    "postcss:watch": "postcss -w src/**/*.pcss --ext scss --dir generated/",
    "prestart": "npm run postcss:build",
    "prebuild": "NODE_ENV=production npm run postcss:build",
    "ng": "ng",
    "start": "ng serve  & npm run postcss:watch",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "npm run build && ngh -d dist/chat-app --no-dotfiles --no-silent --repo git@github.com:ArunMurugan78/anonchat.git --name ArunMurugan78 --email arunmurugan200@gmail.com"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.9",
    "@angular/common": "~11.0.9",
    "@angular/compiler": "~11.0.9",
    "@angular/core": "~11.0.9",
    "@angular/forms": "~11.0.9",
    "@angular/platform-browser": "~11.0.9",
    "@angular/platform-browser-dynamic": "~11.0.9",
    "@angular/router": "~11.0.9",
    "@privacyresearch/libsignal-protocol-typescript": "0.0.12",
    "autoprefixer": "^10.2.3",
    "postcss": "^8.2.4",
    "postcss-short": "^5.0.0",
    "rxjs": "~6.6.0",
    "socket.io-client": "2.3.0",
    "tailwindcss": "^2.0.2",
    "tslib": "^2.0.0",
    "unique-names-generator": "^4.4.0",
    "url-join": "^4.0.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.7",
    "@angular/cli": "~11.0.7",
    "@angular/compiler-cli": "~11.0.9",
    "@types/emscripten": "^1.39.4",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@types/socket.io-client": "^1.4.35",
    "@types/url-join": "^4.0.0",
    "angular-cli-ghpages": "^1.0.0-rc.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.0.0",
    "postcss-cli": "^8.3.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
